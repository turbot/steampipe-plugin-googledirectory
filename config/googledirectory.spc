connection "googledirectory" {
  plugin = "googledirectory"

  # `credential_file` - The path to a JSON credential file that contains service account credentials.
  #credential_file = "/path/to/<public_key_fingerprint>-privatekey.json"

  # `impersonate_user` - The email (string) of the user which should be impersonated. Needs permissions to access the Admin APIs.
  #impersonate_user = "username@domain.com"
}